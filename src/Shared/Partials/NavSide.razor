<div id="kt_app_sidebar" class="app-sidebar">
    <div class="app-sidebar-primary">
        <div class="d-flex flex-column flex-center fs-6 fw-bold px-2 mb-5" id="kt_app_sidebar_primary_header">Rotas</div>
        <div class="app-sidebar-nav flex-grow-1 hover-scroll-overlay-y px-5 pt-2" id="kt_app_sidebar_primary_nav">
            <ul class="nav">
                <li class="nav-item py-2">
                    <a data-bs-toggle="tab" href="" class="nav-link py-4 px-1 btn active">
                        <i class="ki-outline ki-time fs-1"></i>
                        <span class="pt-2 fs-9 fs-lg-7 fw-bold">Suporte</span>
                    </a>
                </li>
                <li class="nav-item py-2">
                    <a data-bs-toggle="tab" href="#app_sidebar_preparing" class="nav-link py-4 px-1 btn">
                        <i class="ki-outline ki-watch fs-1"></i>
                        <span class="pt-2 fs-9 fs-lg-7 fw-bold">Financeiro</span>
                    </a>
                </li>
                <li class="nav-item py-2">
                    <a data-bs-toggle="tab" href="/index" class="nav-link py-4 px-1 btn">
                        <i class="ki-outline ki-delivery-time fs-1"></i>
                        <span class="pt-2 fs-9 fs-lg-7 fw-bold">Tickets</span>
                    </a>
                </li>
            </ul>
        </div>
        <div class="app-sidebar-footer d-flex flex-column flex-center" id="kt_app_sidebar_primary_footer">
            <div class="mb-0">
                <button type="button" class="btn btn-icon btn-color-gray-500 btn-active-color-primary" data-kt-menu-trigger="{default: 'click', lg: 'hover'}" data-kt-menu-overflow="true" data-kt-menu-placement="top-start">
                    <i class="ki-outline ki-code fs-3 me-2"></i>
                    v0.2.1
                </button>
                <div class="menu menu-sub menu-sub-dropdown menu-column menu-rounded menu-gray-800 menu-state-bg-light-primary fw-semibold w-200px" data-kt-menu="true" style="">
                    <div class="menu-item px-3">
                        <div class="menu-content fs-6 text-gray-900 fw-bold px-3 py-4">Changelog v0.2.1</div>
                    </div>
                    <div class="separator mb-3 opacity-75"></div>
                </div>
            </div>
        </div>
